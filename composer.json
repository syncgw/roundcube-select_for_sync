{
	"name" : "syncgw/roundcube-select_for_sync",
	"description" : "Adds the ability to select all adress books, calendars and task lists in RoundCube which should be included in synchronization with sync*gw.",
	"type" : "roundcube-plugin",
	"license": "LGPL-3.0-or-later",
	"authors" : [
		{
			"name" : "Florian Daeumling",
			"homepage" : "https://syncgw.com"
		}
	],
	"keywords" : [
		"addressbook",
		"calendar",
		"tasklist",
		"todo",
		"sync",
		"activesync",
		"caldav",
		"carddav",
		"syncml",
		"synchronization"
	],
	"repositories" : [
	{
			"type" : "composer",
			"url" : "http://plugins.roundcube.net"
		}
	],
	"support" : {
		"issues" : "https://github.com/syncgw/roundcube-select_for_sync/issues",
		"source" : "https://github.com/syncgw/roundcube-select_for_sync"
	},
	"require" : {
		"roundcube/plugin-installer" : ">=0.1.3"
	},
	"extra" : {
		"roundcube" : {
			"min-version" : "0.6.0",
			"max-version" : "10.21",
			"sql-dir" : "SQL"
		}
	}
}